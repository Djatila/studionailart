# Supabase
VITE_SUPABASE_URL=seu_url_do_supabase_aqui
VITE_SUPABASE_ANON_KEY=sua_chave_anonima_do_supabase_aqui
VITE_SUPABASE_SERVICE_ROLE_KEY=sua_service_role_key_aqui

# UAZAPI (os nós do n8n usam {{$env.UAZAPI_KEY}}, e o app pode usar este token para chamadas diretas também)
VITE_UAZAPI_INSTANCE_ID=seu_id_da_instancia_uazapi_aqui
VITE_UAZAPI_TOKEN=seu_token_jwt_aqui

# n8n (Railway - use a URL completa do seu webhook)
VITE_N8N_WEBHOOK_URL=https://seu-projeto.up.railway.app/webhook/seu-webhook-id
VITE_N8N_USERNAME=seu_usuario_n8n_aqui
VITE_N8N_PASSWORD=sua_senha_n8n_aqui